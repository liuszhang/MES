@model DashboardSourceModel

@Html.DevExpress().DashboardViewer(settings => {
    settings.Name = "dashboardViewer1";
    settings.CallbackRouteValues = new { Controller = "Samples", Action = "EUTradeOverviewPartial" };
    settings.ExportRouteValues = new { Controller = "Samples", Action = "EUTradeOverviewPartialExport" };
    settings.AllowExportDashboardItems = true;
    settings.Width = new Unit("100%");
    settings.Height = new Unit("800px");
    settings.ClientSideEvents.ItemWidgetCreated = "function(s, e) { setPieInsidePosition(s, e); }";
    settings.ClientSideEvents.ItemWidgetUpdated = "function(s, e) { setPieInsidePosition(s, e); }";
}).BindToSource(Model).GetHtml()
